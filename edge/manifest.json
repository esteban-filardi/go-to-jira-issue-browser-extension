{
    "name": "Go to Jira Issue",
    "version": "0.0.0.1",
    "manifest_version": 3,
    "description": "An extension to quickly open a browser tab displaying a specific Jira issue.",
    "icons": {
        "16": "icons/16x16.png",
        "32": "icons/32x32.png",
        "48": "icons/48x48.png",
        "128": "icons/128x128.png"
    },
    "permissions": [
        "storage"   
    ],
    "action": {
        "default_popup": "go-to-issue-popup/go-to-issue-popup.html"
    }
}
